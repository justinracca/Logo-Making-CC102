    std::cout << "    JJJ    UU    UU  SS           TT        II     NNNN    NN\n";
    std::cout << "    JJJ    UU    UU   SSSSSS      TT        II     NN NNN  NN\n";
    std::cout << "    JJJ    UU    UU   SSSSSS      TT        II     NN  NNN NN\n";
    std::cout << "JJ  JJJ    UU    UU        SS     TT        II     NN    NNNN\n";
